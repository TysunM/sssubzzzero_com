#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/var/www/sssubzzzero_com/node_modules/.pnpm/vite@7.0.6_@types+node@20.16.11_jiti@2.5.0_lightningcss@1.30.1_tsx@4.20.3_yaml@2.8.0/node_modules/vite/bin/node_modules:/var/www/sssubzzzero_com/node_modules/.pnpm/vite@7.0.6_@types+node@20.16.11_jiti@2.5.0_lightningcss@1.30.1_tsx@4.20.3_yaml@2.8.0/node_modules/vite/node_modules:/var/www/sssubzzzero_com/node_modules/.pnpm/vite@7.0.6_@types+node@20.16.11_jiti@2.5.0_lightningcss@1.30.1_tsx@4.20.3_yaml@2.8.0/node_modules:/var/www/sssubzzzero_com/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/var/www/sssubzzzero_com/node_modules/.pnpm/vite@7.0.6_@types+node@20.16.11_jiti@2.5.0_lightningcss@1.30.1_tsx@4.20.3_yaml@2.8.0/node_modules/vite/bin/node_modules:/var/www/sssubzzzero_com/node_modules/.pnpm/vite@7.0.6_@types+node@20.16.11_jiti@2.5.0_lightningcss@1.30.1_tsx@4.20.3_yaml@2.8.0/node_modules/vite/node_modules:/var/www/sssubzzzero_com/node_modules/.pnpm/vite@7.0.6_@types+node@20.16.11_jiti@2.5.0_lightningcss@1.30.1_tsx@4.20.3_yaml@2.8.0/node_modules:/var/www/sssubzzzero_com/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
